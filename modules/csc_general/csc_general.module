<?php
/**
 * hook_form_alter
 */
function csc_general_form_alter(&$form, &$form_state, $form_id){
  if(isset($form['#node_edit_form'])){
    $types = node_type_get_types();
    $type = $types[$form['type']['#value']]->name;
    $form['title']['#prefix'] = "<h3>Content &nbsp;&gt;&nbsp; <span>$type</span></h3>";
  }
}
